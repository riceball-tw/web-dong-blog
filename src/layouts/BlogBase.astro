---
import type { BaseLayout } from '@/types';
import Navbar from '@/components/organisms/global-navbar/GlobalNavbar.astro';
import Footer from '@/components/app/Footer.astro';
import Base from '@/layouts/Base.astro';

export interface Props extends BaseLayout {}
---

<Base {...Astro.props}>
